{
  "short_name": "Chain Desktop Wallet",
  "name": "Chain Desktop Wallet",
  "icons": {
    "128": "logo192.png",
    "180": "logo192.png"
  },
  "manifest_version": 2,
  "version": "2.4.4",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "options_ui": {
    "page": "index.html"
  },
  "web_accessible_resources": [
    "inpage.js"
  ],
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}